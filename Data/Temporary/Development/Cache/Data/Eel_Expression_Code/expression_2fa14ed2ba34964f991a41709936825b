return function ($context) {return $context->getAndWrap('Array')->callAndWrap('push', array($context->getAndWrap('value'),$context->getAndWrap('String')->callAndWrap('toLowerCase', array($context->getAndWrap('String')->callAndWrap('pregReplace', array($context->getAndWrap('node')->getAndWrap('nodeType')->getAndWrap('name'),'/[[:^alnum:]]/','-'))))));};